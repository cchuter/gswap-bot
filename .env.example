# Copy to .env and fill with your credentials
# Encrypted private key payload from GSwap dashboard
PRIVATE_KEY_ENCRYPTED=your_encrypted_private_key_payload
# Decrypted private key in hex format
PRIVATE_KEY=your_private_key_hex
# Wallet address (checksummed)
WALLET_ADDRESS=0xYourWalletAddress
# GalaChain client identity string
GALACHAIN_ADDRESS=client|your_client_id
